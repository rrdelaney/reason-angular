  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "lib",
    "strict": true,
    "allowJs": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "lib": [
      "es2015",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableIvy": true
  }
}